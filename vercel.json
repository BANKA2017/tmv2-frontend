{
  "redirects": [
    {
      "source": "/api/v2/(.*)",
      "destination": "https://tmv2api2.bangdream.fun/api/v2/$1",
      "permanent": false
    },{
      "source": "/api/v3/(.*)",
      "destination": "https://tmapi.nest.moe/api/v3/$1",
      "permanent": false
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [{
    "source": "/(.*)",
    "headers": [{
      "key": "Permissions-Policy",
      "value": "interest-cohort=()"
    }]
  }]
}
